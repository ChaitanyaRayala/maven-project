pipeline{
    agent any
    tools{
        maven 'localMaven'
        jdk 'C:\Program Files\Java\jdk1.8.0_161'
    }
    stages{
        stage('Build'){
            steps{
                bat 'mvn clean install'
            }
            post{
                success{
                    echo 'Now Archiving...'
                    archiveArtifacts artifacts:'**/target/*.war'
                }
            }
        }
    }
}