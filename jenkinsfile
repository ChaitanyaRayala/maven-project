pipeline{
    agent any
    tools{
        maven 'localMaven'
        jdk 'localJdk'
    }
    stages{
        stage('Build'){
            steps{
                bat 'mvn clean install'
            }
            post{
                success{
                    echo 'Now Archiving...'
                    archiveArtifacts artifacts:'**/target/*.war'
                }
            }
        }
        stage('Deploy to staging'){
            steps{
                build job:'Deploy-to-staging'
                build job:'static-analysis-job'
            }
        }
    }
}